# DealerVait Environment Configuration
# Copy this file to .env and update with your values

# API Configuration
API_BASE_URL=https://dcrnapi.azurewebsites.net/api/
SOCKET_URL=https://dcrnapi.azurewebsites.net

# Staging API (for staging builds)
STAGING_API_BASE_URL=https://dcgptrnapi.azurewebsites.net/api/
STAGING_SOCKET_URL=https://dcgptrnapi.azurewebsites.net

# Development API (local development only)
# DEV_API_BASE_URL=http://localhost:3000/api/
# DEV_SOCKET_URL=http://localhost:3000

# Build Configuration
NODE_ENV=production

# App Configuration
APP_NAME=DealerVait
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Security (DO NOT commit real values)
# DEALERVAIT_UPLOAD_STORE_PASSWORD=your_keystore_password
# DEALERVAIT_UPLOAD_KEY_PASSWORD=your_key_password
# DEALERVAIT_UPLOAD_KEY_ALIAS=dealervait
# DEALERVAIT_UPLOAD_STORE_FILE=app/my-release-key.keystore

# Test Credentials (for development testing only)
TEST_USERNAME=aitest
TEST_PASSWORD=1234
TEST_URL=https://yahauto.autodealerscloud.com/
