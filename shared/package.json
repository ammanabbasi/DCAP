{
  "name": "@dealerscloud/shared",
  "version": "1.0.0",
  "description": "Shared types, utilities, and constants for DealersCloud platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:changed": "jest --changedSince=HEAD~1",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dealerscloud",
    "shared",
    "types",
    "utilities",
    "automotive"
  ],
  "author": "DealersCloud Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}