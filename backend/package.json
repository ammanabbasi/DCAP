{
  "name": "dealerscloud-backend",
  "version": "1.0.0",
  "description": "DealersCloud Backend API - Express.js server for automotive dealership management platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
