  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.13.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "socket.io": "^4.8.1",
    "knex": "^3.1.0",
    "mssql": "^11.0.1",
    "redis": "^4.7.0",
    "azure-storage": "^2.10.7",
    "@sendgrid/mail": "^8.1.3",
